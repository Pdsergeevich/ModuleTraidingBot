# Создайте файл .env и заполните эти значения

# Telegram API (получить на https://my.telegram.org/auth)
TELEGRAM_API_ID=your_api_id_here
TELEGRAM_API_HASH=your_api_hash_here

# Tinkoff Invest API (получить в личном кабинете)
TINKOFF_TOKEN=your_tinkoff_token_here
TINKOFF_ACCOUNT_ID=your_account_id_here

# OpenAI API
OPENAI_API_KEY=your_openai_key_here

# Anthropic API (опционально)
ANTHROPIC_API_KEY=your_anthropic_key_here
